{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}",
          "path": "../TaskScheduler/Logs/TaskSchedulersrv_.log",
          "rollingInterval": "Day",
          "fileSizeLimitBytes": 50000000,
          "rollOnFileSizeLimit": true
        }
      }
    ],
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "AllowedHosts": "*",
  "BearerTokensSettings": {
    "Key": "",
    "Issuer": "https://localhost:5001/",
    "AccessTokenExpirationMinutes": 1440,
    "RefreshTokenExpirationMinutes": 720
  },
  "AppSettings": {
    "Host": "localhost",
    "AllowedAuthOrigins": [ "http://localhost:8080", "http://localhost:5001", "http://localhost:52407", "http://localhost:44385" ],
    "Secret": "",
    "Policy": [ "Controller", "Administrator", "Mentor", "Manager", "Agent", "Broker", "Call-centre", "Call—entre" ]
  },
  "BackgroundSettings": {
    "Timed": 10,
    "Enabled": true
  }
}
